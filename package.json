{
    "name": "ocamlbuild",
    "optionalDependencies": {},
    "exportedEnvVars": {
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.9.2"
        },
        "ocamlbuild_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.9.2"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        }
    },
    "version": "0.9.2",
    "dependencies": {
        "substs": "https://github.com/yunxing/substs.git",
        "nopam": "https://github.com/yunxing/nopam.git",
        "ocaml": "https://github.com/npm-opam/ocaml.git#npm-4.02.3",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git",
        "dependency-env": "https://github.com/npm-ml/dependency-env.git"
    },
    "scripts": {
        "postinstall": "eval $(dependencyEnv) && nopam && make -f configure.make Makefile.config OCAMLBUILD_PREFIX=$opam_prefix OCAMLBUILD_BINDIR=$opam_bin OCAMLBUILD_LIBDIR=$opam_lib OCAML_NATIVE=true OCAML_NATIVE_TOOLS=true && make check-if-preinstalled all opam-install && (opam-installer --prefix=$opam_prefix || true) && injectOpamEnvs"
    }
}
